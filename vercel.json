{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "server.js"
        },
        {
            "src": "/all",
            "dest": "server.js"
        },
        {
            "src": "/form",
            "dest": "server.js"
        },
        {
            "src": "/*",
            "dest": "server.js"
        }
    ]
}
